{
    "name": "@bytechef/embedded-react",
    "version": "0.1.0",
    "description": "ByteChef Embedded React SDK.",
    "type": "module",
    "scripts": {
        "build": "tsc && vite build",
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
        "format": "prettier --check .",
        "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\" && eslint src --max-warnings=0 --fix",
        "stylelint": "stylelint **/*.css",
        "stylelint:fix": "stylelint **/*.css --fix",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:watch": "vitest --watch",
        "coverage": "vitest run --coverage",
        "watch": "tsc && vite build --watch",
        "publish:local": "npm publish --registry http://localhost:4873",
        "link:local": "npm link",
        "unlink:local": "npm unlink -g @bytechef/embedded-react"
    },
    "module": "./dist/main.js",
    "exports": "./dist/main.js",
    "files": [
        "dist"
    ],
    "types": "./dist/main.d.ts",
    "peerDependencies": {
        "react": "^19.2.3",
        "react-dom": "^19.2.3"
    },
    "author": "Kresimir Coko <kresimir.coko@bytechef.io>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/bytechefhq/bytechef.git",
        "directory": "sdks/frontend/embedded/library/react"
    },
    "keywords": [
        "bytechef",
        "embedded",
        "workflow",
        "automation",
        "react",
        "sdk"
    ],
    "devDependencies": {
        "@chromatic-com/storybook": "4.1.3",
        "@eslint/js": "9.21.0",
        "@storybook/react": "10.1.11",
        "@storybook/react-vite": "10.1.11",
        "@tailwindcss/vite": "4.1.18",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "16.2.0",
        "@types/node": "22.13.8",
        "@types/react": "19.1.6",
        "@types/react-dom": "19.1.6",
        "@vitejs/plugin-react": "5.1.2",
        "@vitest/coverage-v8": "4.0.16",
        "@vitest/ui": "4.0.16",
        "clsx": "^2.1.1",
        "eslint": "9.31.0",
        "eslint-config-prettier": "10.1.5",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-react-refresh": "0.4.19",
        "eslint-plugin-storybook": "10.1.11",
        "glob": "13.0.0",
        "globals": "16.0.0",
        "jsdom": "26.0.0",
        "prettier": "3.5.2",
        "storybook": "^10.1.11",
        "stylelint": "16.15.0",
        "stylelint-config-standard": "37.0.0",
        "tailwind-merge": "^3.4.0",
        "tailwindcss": "4.1.18",
        "tw-animate-css": "^1.4.0",
        "typescript": "5.7.3",
        "typescript-eslint": "8.25.0",
        "vite": "^7.3.1",
        "vite-plugin-dts": "4.5.3",
        "vite-plugin-lib-inject-css": "2.2.1",
        "vitest": "4.0.16"
    }
}
