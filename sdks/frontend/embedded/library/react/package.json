{
    "name": "@bytechef/embedded-react",
    "version": "0.1.0",
    "description": "ByteChef Embedded React SDK.",
    "type": "module",
    "scripts": {
        "build": "tsc && vite build",
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
        "format": "prettier --check .",
        "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\" && eslint src --max-warnings=0 --fix",
        "stylelint": "stylelint **/*.css",
        "stylelint:fix": "stylelint **/*.css --fix",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:watch": "vitest --watch",
        "coverage": "vitest run --coverage",
        "watch": "tsc && vite build --watch",
        "yalc:publish": "npm run build && yalc publish",
        "yalc:push": "npm run build && yalc push",
        "dev:yalc": "concurrently \"npm run watch\" \"nodemon --watch dist --delay 1 --exec 'yalc push'\""
    },
    "module": "./dist/main.js",
    "exports": "./dist/main.js",
    "files": [
        "dist"
    ],
    "types": "./dist/main.d.ts",
    "peerDependencies": {
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
    },
    "author": "Kresimir Coko <kresimir.coko@bytechef.io>",
    "license": "MIT",
    "devDependencies": {
        "@chromatic-com/storybook": "3.2.5",
        "@eslint/js": "9.21.0",
        "@storybook/addon-essentials": "8.6.3",
        "@storybook/addon-interactions": "8.6.3",
        "@storybook/addon-links": "8.6.3",
        "@storybook/addon-onboarding": "8.6.3",
        "@storybook/blocks": "8.6.3",
        "@storybook/react": "8.6.3",
        "@storybook/react-vite": "8.6.3",
        "@storybook/test": "8.6.3",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "16.2.0",
        "@types/node": "22.13.8",
        "@types/react": "19.1.6",
        "@types/react-dom": "19.1.6",
        "@vitejs/plugin-react": "4.3.4",
        "@vitest/coverage-v8": "3.0.7",
        "@vitest/ui": "3.0.7",
        "clsx": "2.1.1",
        "concurrently": "^8.2.2",
        "eslint": "9.31.0",
        "eslint-config-prettier": "10.1.5",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-react-refresh": "0.4.19",
        "eslint-plugin-storybook": "0.11.3",
        "glob": "11.0.1",
        "globals": "16.0.0",
        "jsdom": "26.0.0",
        "nodemon": "^3.1.0",
        "prettier": "3.5.2",
        "storybook": "8.6.3",
        "stylelint": "16.15.0",
        "stylelint-config-standard": "37.0.0",
        "typescript": "5.7.3",
        "typescript-eslint": "8.25.0",
        "vite": "6.2.0",
        "vite-plugin-dts": "4.5.1",
        "vite-plugin-lib-inject-css": "2.2.1",
        "vitest": "3.0.7"
    }
}
