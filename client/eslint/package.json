        "eslint-plugin-eslint-plugin": "^6.2.0",
        "eslint-plugin-n": "^17.10.3",
        "globals": "^15.13.0",
        "mocha": "^10.8.2",
        "npm-run-all": "^4.1.5"
    },
    "engines": {
        "node": ">=20.19"
    },
    "license": "ISC"
}
