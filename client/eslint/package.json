{
    "name": "eslint-plugin-bytechef",
    "version": "1.1.0",
    "description": "Bytechef ESLint plugin",
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin"
    ],
    "author": "Bytechef",
    "main": "./lib/index.js",
