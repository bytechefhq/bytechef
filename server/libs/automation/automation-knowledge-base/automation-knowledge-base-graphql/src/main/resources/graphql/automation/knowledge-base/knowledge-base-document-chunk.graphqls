extend type Mutation {
    updateKnowledgeBaseDocumentChunk(id: ID!, knowledgeBaseDocumentChunk: KnowledgeBaseDocumentChunkInput!): KnowledgeBaseDocumentChunk
    deleteKnowledgeBaseDocumentChunk(id: ID!): Boolean
}

type KnowledgeBaseDocumentChunk {
    id: ID!
