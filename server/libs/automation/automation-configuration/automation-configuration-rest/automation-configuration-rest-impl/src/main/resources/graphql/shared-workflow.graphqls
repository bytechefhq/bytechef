extend type Query {
    getSharedWorkflow(workflowUuid: String!): SharedWorkflow!
}

extend type Mutation {
    exportSharedWorkflow(workflowId: String!, templateDescription: String): String!
    importSharedWorkflow(workflowUuid: String!, projectId: ID!): Boolean!
}

type SharedWorkflow {
    workflowProjectInfo: WorkflowProjectInfo
    workflow: Workflow
}

type WorkflowProjectInfo {
    projectVersion: Int!
    templateDescription: String!
}
