          "required": true,
          "defaultValue": 0
        },
        {
          "description": "The maximum possible generated value.",
          "name": "endInclusive",
          "type": "INTEGER",
          "required": true,
          "defaultValue": 100
        }
      ]
    },
    {
      "display": {
        "description": "Generates a random float value.",
        "label": "Float"
      },
      "name": "randomFloat"
    }
