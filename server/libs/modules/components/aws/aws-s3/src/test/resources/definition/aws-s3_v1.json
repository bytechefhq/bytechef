{
  "display": {
    "description": "AWS S3 is a simple object storage service provided by Amazon Web Services.",
    "label": "AWS S3"
  },
  "name": "aws-s3",
  "actions": [
    {
      "display": {
        "description": "Get the AWS S3 object.",
        "label": "Get Object"
      },
      "name": "getObject",
      "outputSchema": [
        {
          "type": "OBJECT",
          "objectType": "FILE_ENTRY",
          "properties": [
            {
              "name": "extension",
              "required": true,
              "type": "STRING"
            },
            {
              "name": "mimeType",
              "required": true,
              "type": "STRING"
            },
            {
              "name": "name",
              "required": true,
              "type": "STRING"
            },
            {
              "name": "url",
              "required": true,
              "type": "STRING"
            }
          ]
        }
      ],
      "properties": [
        {
          "description": "The AWS S3 uri.",
          "label": "URI",
          "name": "uri",
          "required": true,
          "type": "STRING"
        },
        {
          "description": "Filename to set for binary data.",
          "label": "Filename",
          "name": "filename",
          "required": true,
          "type": "STRING",
          "defaultValue": "file.xml"
        }
      ]
    },
    {
      "display": {
        "description": "Get the url of an AWS S3 object.",
        "label": "Get URL"
      },
      "name": "getUrl",
      "outputSchema": [
        {
          "type": "STRING"
        }
      ],
      "properties": [
        {
          "description": "The AWS S3 uri.",
          "label": "URI",
          "name": "uri",
          "required": true,
          "type": "STRING"
        }
      ]
    },
    {
      "display": {
        "description": "Get the list AWS S3 objects.",
        "label": "List Objects"
      },
      "name": "listObjects",
      "outputSchema": [
        {
          "type": "ARRAY",
          "items": [
            {
              "type": "OBJECT",
              "properties": [
                {
                  "name": "key",
                  "type": "STRING"
                },
                {
                  "name": "suffix",
                  "type": "STRING"
                },
                {
                  "name": "uri",
                  "type": "STRING"
                }
              ]
            }
          ]
        }
      ],
      "properties": [
        {
          "description": "The bucket to list AWS S3 objects from.",
          "label": "Bucket",
          "name": "bucket",
          "required": true,
          "type": "STRING"
        },
        {
          "description": "The prefix of an AWS S3 objects.",
          "label": "Prefix",
          "name": "prefix",
          "required": true,
          "type": "STRING"
        }
      ]
    },
    {
      "display": {
        "description": "Get the url of an pre-signed AWS S3 object.",
        "label": "Get Pre-signed Object"
      },
      "name": "presignGetObject",
      "outputSchema": [
        {
          "type": "STRING"
        }
      ],
      "properties": [
        {
          "description": "The AWS S3 uri.",
          "label": "URI",
          "name": "uri",
          "required": true,
          "type": "STRING"
        }
      ]
    },
    {
      "display": {
        "description": "Store an object to AWS S3.",
        "label": "Put Object"
      },
      "name": "putObject",
      "outputSchema": [
        {
          "type": "STRING"
        }
      ],
      "properties": [
        {
          "description": "The AWS S3 uri.",
          "label": "URI",
          "name": "uri",
          "required": true,
          "type": "STRING"
        },
        {
          "description": "The object property which contains a reference to the file that needs to be written to AWS S3.",
          "label": "File",
          "name": "fileEntry",
          "required": true,
          "type": "OBJECT",
          "objectType": "FILE_ENTRY",
          "properties": [
            {
              "name": "extension",
              "required": true,
              "type": "STRING"
            },
            {
              "name": "mimeType",
              "required": true,
              "type": "STRING"
            },
            {
              "name": "name",
              "required": true,
              "type": "STRING"
            },
            {
              "name": "url",
              "required": true,
              "type": "STRING"
            }
          ]
        },
        {
          "description": "The canned ACL to apply to the object.",
          "label": "ACL",
          "name": "acl",
          "type": "STRING"
        }
      ]
    }
  ],
  "version": 1
}
