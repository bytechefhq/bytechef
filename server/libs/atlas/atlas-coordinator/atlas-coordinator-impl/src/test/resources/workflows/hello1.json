{
  "label": "Hello Demo",
  "inputs": [
    {
      "name": "yourName",
      "label": "Your Name",
      "type": "STRING",
      "required": true
    }
  ],
  "outputs": [
    {
      "name": "myMagicNumber",
      "value": "${randomNumber}"
    }
  ],
  "tasks": [
    {
      "name": "randomNumber",
      "label": "Generate a random number",
      "type": "randomHelper/v1/randomInt",
      "startInclusive": 1000,
      "endInclusive": 5000
    }
  ]
}
