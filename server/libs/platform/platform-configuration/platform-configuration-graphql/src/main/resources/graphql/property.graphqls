type Option {
    description: String
    label: String
    value: Map
}

type OptionsDataSource {
    optionsLookupDependsOn: [String!]
}

type PropertiesDataSource {
    propertiesLookupDependsOn: [String!]
}

enum PropertyType {
    ARRAY
    BOOLEAN
    DATE
    DATE_TIME
    DYNAMIC_PROPERTIES
    FILE_ENTRY
    INTEGER
    NULL
    NUMBER
    STRING
    OBJECT
    TASK
    TIME
}

enum ControlType {
    ARRAY_BUILDER
    CODE_EDITOR
    DATE
    DATE_TIME
    EMAIL
    INTEGER
    JSON_SCHEMA_BUILDER
    FILE_ENTRY
    MULTI_SELECT
    NUMBER
    NULL
    OBJECT_BUILDER
    PASSWORD
    PHONE
    RICH_TEXT
    SELECT
    TEXT
    TEXT_AREA
    TIME
    URL
}
