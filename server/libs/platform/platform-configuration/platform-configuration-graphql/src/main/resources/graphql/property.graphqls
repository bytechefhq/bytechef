interface Property {
    advancedOption: Boolean
    description: String
    displayCondition: String
    expressionEnabled: Boolean
    hidden: Boolean
    name: String
    required: Boolean
    type: PropertyType!
}

type ArrayProperty implements Property {
    advancedOption: Boolean
    description: String
    displayCondition: String
    expressionEnabled: Boolean
    hidden: Boolean
    name: String
    required: Boolean
    type: PropertyType!

    controlType: ControlType!
    defaultValue: [Map]
    exampleValue: [Map]
    items: [Property!]
    label: String
    maxItems: Long
    minItems: Long
    multipleValues: Boolean
    options: [Option!]
    optionsDataSource: OptionsDataSource
    placeholder: String
}

