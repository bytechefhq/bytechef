{
  "label": "Local File - WRITE operation",
  "inputs": [
    {
      "name": "fileEntry",
      "label": "File property",
      "type": "FILE_ENTRY",
      "required": true
    },
    {
      "name": "fileName",
      "label": "Absolute file path to write to",
      "type": "STRING",
      "required": true
    }
  ],
  "outputs": [
    {
      "name": "writeLocalFile",
      "value": "${writeLocalFile}"
    }
  ],
  "tasks": [
    {
      "name": "writeLocalFile",
      "label": "Write to the local file",
      "type": "localFile/1.0/write",
      "fileEntry": "${fileEntry}",
      "fileName": "${fileName}"
    }
  ]
}
