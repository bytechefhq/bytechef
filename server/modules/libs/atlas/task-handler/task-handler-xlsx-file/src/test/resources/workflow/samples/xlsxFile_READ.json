{
  "label": "XLSX File - READ operation",
  "inputs": [
    {
      "name": "fileEntry",
      "label": "File property",
      "type": "FILE_ENTRY",
      "required": true
    }
  ],
  "outputs": [
    {
      "name": "readXlsxFile",
