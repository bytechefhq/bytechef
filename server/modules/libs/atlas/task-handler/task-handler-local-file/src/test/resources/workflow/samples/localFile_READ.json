{
  "label": "Local File - READ operation",
  "inputs": [
    {
      "name": "fileName",
      "label": "Absolute file path to read from",
      "type": "string",
      "required": true
    }
  ],
  "outputs": [
    {
      "name": "readLocalFile",
      "value": "${readLocalFile}"
    }
  ],
  "tasks": [
    {
      "name": "readLocalFile",
      "label": "Read from local file",
      "type": "localFile",
      "operation": "READ",
      "fileName": "${fileName}"
    }
  ]
}
