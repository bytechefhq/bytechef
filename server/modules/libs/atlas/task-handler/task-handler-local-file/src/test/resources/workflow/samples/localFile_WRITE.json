{
  "label": "Local File - WRITE operation",
  "inputs": [
    {
      "name": "fileEntry",
      "label": "File property",
      "type": "JSON",
      "required": true
    },
    {
      "name": "fileName",
      "label": "Absolute file path to write t",
      "type": "string",
      "required": true
    }
  ],
  "outputs": [
    {
      "name": "writeToFile",
      "value": "${writeToFile}"
    }
  ],
  "tasks": [
    {
      "name": "writeToFile",
      "label": "Write to file",
      "type": "localFile",
      "operation": "WRITE",
      "fileEntry": "${fileEntry}",
      "fileName": "${fileName}"
    }
  ]
}
