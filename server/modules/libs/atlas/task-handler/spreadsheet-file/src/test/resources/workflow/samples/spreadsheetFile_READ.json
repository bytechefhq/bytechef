{
  "label": "Spreadsheet File - READ operation",
  "inputs": [
    {
      "name": "fileEntry",
      "label": "File property",
      "type": "JSON",
      "required": true
    }
  ],
  "outputs": [
    {
      "name": "readFromSpreadsheet",
      "value": "${readFromSpreadsheet}"
    }
  ],
  "tasks": [
    {
      "name": "readFromSpreadsheet",
      "label": "Read from the spreadsheet file",
      "type": "spreadsheetFile",
      "operation": "READ",
      "fileEntry": "${fileEntry}",
      "includeEmptyCells": true
    }
  ]
}
